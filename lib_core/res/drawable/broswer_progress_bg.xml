<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/lib_corner_big" />
			<solid android:color="@color/lib_color_broswer_progress_bg"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="@dimen/lib_corner_big" />
				<solid android:color="@color/lib_color_broswer_progress"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/lib_corner_big" />
                <solid android:color="@color/lib_color_broswer_progress"/>
            </shape>
        </clip>
    </item>

</layer-list>